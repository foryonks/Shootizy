{
  "name": "shootizy-client",
  "version": "0.1.0",
  "private": true,
  "icons": {
    "jsonFilePath": "src/scripts/Components/Icon/Icons.json",
    "svgFolder": "src/assets/icons"
  },
  "scripts": {
    "build": "cross-env NODE_PATH=src/ react-scripts build && rm -fr ../shootizy-server/public/ && mv build ../shootizy-server/public",
    "start": "npm-run-all -p start-js create-icons-watch",
    "start-debug": "cross-env REACT_APP_DEBUG=true npm-run-all -p start-js create-icons-watch",
    "start-js": "cross-env NODE_PATH=src/ react-scripts start",
    "test": "cross-env NODE_PATH=src/ react-scripts test",
    "create-icons": "node ./tasks/icons-to-json.js",
    "create-icons-watch": "npm run create-icons -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "dependencies": {
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "animated-scroll-to": "^2.0.5",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "destyle": "^1.1.4",
    "destyle.css": "^1.0.11",
    "emotion": "^10.0.27",
    "interweave": "^12.1.1",
    "jodit-react": "^1.0.33",
    "load-script": "^1.0.0",
    "lodash.throttle": "^4.1.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "polyfill-array-includes": "^2.0.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-breadcrumbs": "^2.1.7",
    "react-calendar": "^2.19.2",
    "react-columned": "^1.1.3",
    "react-date-picker": "^7.10.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-media-query": "^1.0.2",
    "react-lazyload": "^2.6.5",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "dom-parser": "^0.1.6",
    "eslint-plugin-react-hooks": "^2.3.0",
    "node-watch": "^0.6.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:3001"
}
